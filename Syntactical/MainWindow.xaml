<Window
    x:Class="Syntactical.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Syntactical"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:progresspanel="clr-namespace:Syntactical.ProgressPanel"
    Title="Dog"
    Width="800"
    Height="500"
    d:DataContext="{d:DesignInstance Type=local:MainWindowViewModel}"
    mc:Ignorable="d">
    <Grid>
        <progresspanel:ProgressPanelControl DataContext="{Binding ProgressPanelViewModel}" />
    </Grid>
</Window>
